Object subclass: #Accion	instanceVariableNames: ''	classVariableNames: ''	poolDictionaries: ''	category: 'tp1-cuoora'!Object subclass: #Cuoora	instanceVariableNames: 'users topics questions'	classVariableNames: ''	poolDictionaries: ''	category: 'tp1-cuoora'!!Cuoora methodsFor: 'as yet unclassified' stamp: 'LucasB 3/27/2019 23:37'!searchQuestionsFrom: username	^ self questions select: [ :q | q name = username ]! !!Cuoora methodsFor: 'as yet unclassified' stamp: 'LucasB 3/24/2019 17:01'!addTopic: unTopico	self topics add: unTopico! !!Cuoora methodsFor: 'as yet unclassified' stamp: 'LucasB 3/24/2019 17:01'!questions	^ questions! !!Cuoora methodsFor: 'as yet unclassified' stamp: 'LucasB 3/27/2019 19:34'!follow: fromUser follower: anotherUser	fromUser follow: anotherUser! !!Cuoora methodsFor: 'as yet unclassified' stamp: 'LucasB 3/24/2019 17:02'!topics	^ topics ! !!Cuoora methodsFor: 'as yet unclassified' stamp: 'LucasB 3/24/2019 13:36'!topicWithName: tname andDescription: tdesc	self addTopic: (Topico newTopic: tname desc: tdesc)! !!Cuoora methodsFor: 'as yet unclassified' stamp: 'LucasB 3/24/2019 15:57'!questionWithTitle: titulo withTopics: topicos andDescription: text fromUser: username	self		addQuestion:			(Pregunta				questionWithTitle: titulo				withTopics: topicos				andDescription: text				fromUser: username)! !!Cuoora methodsFor: 'as yet unclassified' stamp: 'LucasB 3/28/2019 13:55'!interestingQuestions: fromUser |myInterestingQuestions|	myInterestingQuestions := self questions select: [ :q | q topics includesAny: fromUser myTopics ].	myInterestingQuestions 	addAll: [ fromUser followers select: [ :f | self searchQuestionsFrom: f ] ].! !!Cuoora methodsFor: 'as yet unclassified' stamp: 'LucasB 3/26/2019 15:19'!searchAnswersFrom: username	^ self questions do: [ :q | q obtainAnswers select: [ :a | a name = username ] ] ! !!Cuoora methodsFor: 'as yet unclassified' stamp: 'LucasB 3/24/2019 17:01'!initialize	users := Set new.	topics := Set new.	questions := Set new.! !!Cuoora methodsFor: 'as yet unclassified' stamp: 'LucasB 3/24/2019 13:04'!createUser: username withPass: passwd	users add: (Usuario createUser: username withPass: passwd)! !!Cuoora methodsFor: 'as yet unclassified' stamp: 'LucasB 3/24/2019 15:56'!addQuestion: aQuestion	questions add: aQuestion! !Object subclass: #Pregunta	instanceVariableNames: 'topics title actions creation text user answers'	classVariableNames: ''	poolDictionaries: ''	category: 'tp1-cuoora'!!Pregunta methodsFor: 'as yet unclassified' stamp: 'LucasB 3/24/2019 16:06'!initialize	actions := Set new.	creation := Date today.	answers := Set new! !!Pregunta methodsFor: 'as yet unclassified' stamp: 'LucasB 3/26/2019 15:19'!obtainAnswers	^ answers! !!Pregunta methodsFor: 'as yet unclassified' stamp: 'LucasB 3/24/2019 16:57'!questionWithTitle: titulo withTopics: topicos andDescription: texto fromUser: username	title := titulo.	topics := topicos.	text := texto.	user := username.	^ self! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Pregunta class	instanceVariableNames: ''!!Pregunta class methodsFor: 'creation' stamp: 'LucasB 3/24/2019 16:57'!questionWithTitle: titulo withTopics: topicos andDescription: texto fromUser: username	^ self new		questionWithTitle: titulo		withTopics: topicos		andDescription: texto		fromUser: username! !Object subclass: #Respuesta	instanceVariableNames: 'question actions creation text user'	classVariableNames: ''	poolDictionaries: ''	category: 'tp1-cuoora'!!Respuesta methodsFor: 'initialization' stamp: 'LucasB 3/24/2019 16:46'!initialize	actions := Set new.	creation := Date today.! !Object subclass: #Topico	instanceVariableNames: 'name description'	classVariableNames: ''	poolDictionaries: ''	category: 'tp1-cuoora'!!Topico methodsFor: 'instance creation' stamp: 'LucasB 3/24/2019 13:26'!newWithName: tname andDesciption: tdescription	name:= tname.	description:= tdescription.	^ self! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Topico class	instanceVariableNames: ''!!Topico class methodsFor: 'instance creation' stamp: 'LucasB 3/24/2019 13:26'!newTopic: tname desc: tdescription	self new newWithName: tname andDesciption: tdescription! !Object subclass: #Usuario	instanceVariableNames: 'username passwd creation mytopics follows'	classVariableNames: ''	poolDictionaries: ''	category: 'tp1-cuoora'!!Usuario methodsFor: 'instance creation' stamp: 'LucasB 3/24/2019 13:08'!generateQuestion: titulo withTopics: topicos andDescription: texto	(Cuoora		questionWithTitle: titulo		withTopics: topicos		andDescription: texto		fromUser: self)! !!Usuario methodsFor: 'instance creation' stamp: 'LucasB 3/26/2019 15:43'!myTopics	^ mytopics ! !!Usuario methodsFor: 'instance creation' stamp: 'LucasB 3/22/2019 15:26'!newWithName: name andPass: password	username := name.	creation := Date today.	passwd := password.	mytopics := Set new.	^ self! !!Usuario methodsFor: 'instance creation' stamp: 'LucasB 3/22/2019 15:35'!crearPregunta: titulo conTopicos: topicos yDescripcion: texto	Cuoora		questionWithTitle: titulo		withTopics: topicos		andDescription: texto		fromUser: self! !!Usuario methodsFor: 'instance creation' stamp: 'LucasB 3/24/2019 15:43'!initialize	mytopics := Set new.	follows := Set new.! !!Usuario methodsFor: 'instance creation' stamp: 'LucasB 3/27/2019 20:06'!followers	^ follows ! !!Usuario methodsFor: 'instance creation' stamp: 'LucasB 3/24/2019 15:48'!follow: anotherUser	follows add: anotherUser! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Usuario class	instanceVariableNames: ''!!Usuario class methodsFor: 'initialization' stamp: 'LucasB 3/24/2019 13:04'!createUser: username withPass: passwd	^ self new newWithName: username andPass: passwd! !